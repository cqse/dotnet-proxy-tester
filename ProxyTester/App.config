<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.net>
    <!-- By default, the upload daemon uses the IE proxy settings.
         See https://docs.microsoft.com/en-us/dotnet/framework/configure-apps/file-schema/network/defaultproxy-element-network-settings
    -->

    <!-- Uncomment the following XML to make the uploader use a custom proxy selection script.
    <defaultProxy>
      <proxy usesystemdefault="False" scriptLocation="http://your-proxy.com/script.pac" />
    </defaultProxy>
    -->

    <!-- Uncomment the following XML to make the uploader use a custom proxy.
    <defaultProxy>
      <proxy bypassonlocal="True" proxyaddress="http://proxy.domain.org:8888/" />
    </defaultProxy>
    -->

    <!-- Uncomment the following XML to make the uploader ignore the system proxy and NOT use any proxy.
    <defaultProxy>
      <proxy usesystemdefault="False" />
    </defaultProxy>
    -->
  </system.net>
</configuration>